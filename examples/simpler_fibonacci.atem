main: func () -> Int64 = {
    result: var Int64 = fibonacci(int_v1)
    return 0
}

fibonacci: func (n: Int64) -> Int64 = 
{
    return if n <= 2 {
        1
    }
    else {
        prev1: var Int64 = 1
        prev2: var Int64 = 1
        result: var Int64 = 0
        for i <= n with i: var Int64 = 0 step i += 1 {
            result = prev1 + prev2
            prev1 = prev2
            prev2 = result
        }
        result
    }
}
